<!DOCTYPE html>
<html>
<head>



    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>AdminLTE 2 | Dashboard</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.6 -->
    <link rel="stylesheet" href="~/assetsWeb/bootstrap/css/bootstrap.min.css" runat="server">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css" runat="server">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css" runat="server">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/assetsWeb/dist/css/AdminLTE.min.css" runat="server">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->

    <link rel="stylesheet" href="~/assetsWeb/dist/css/skins/_all-skins.min.css" runat="server">
   
    <!-- iCheck -->
    <link rel="stylesheet" href="~/assetsWeb/plugins/iCheck/flat/blue.css" runat="server">
    <!-- Morris chart -->
    <link rel="stylesheet" href="~/assetsWeb/plugins/morris/morris.css" runat="server">
    <!-- jvectormap -->
    <link rel="stylesheet" href="~/assetsWeb/plugins/jvectormap/jquery-jvectormap-1.2.2.css" runat="server">
    <!-- Date Picker -->
    <link rel="stylesheet" href="~/assetsWeb/plugins/datepicker/datepicker3.css" runat="server">
    <!-- Daterange picker -->
    <link rel="stylesheet" href="~/assetsWeb/plugins/daterangepicker/daterangepicker.css" runat="server">
    <!-- bootstrap wysihtml5 - text editor -->
    <link rel="stylesheet" href="~/assetsWeb/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css" runat="server">



</head>
<body>
    
   
    @Html.Partial("~/Views/Shared/decideur/tous/navDroite.cshtml")
 

  


    <div class="wrapper">

        @Html.Partial("~/Views/Shared/decideur/tous/menu.cshtml")

        <!-- Left side column. contains the logo and sidebar -->
        @Html.Partial("~/Views/Shared/decideur/ResponsableCommune/navGauche.cshtml")

 
        <div class="content-wrapper">
            
           
                <!-- Main content -->
                @RenderBody()


                <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer"></footer>

        <!-- Control Sidebar -->
        @Html.Partial("~/Views/Shared/decideur/tous/navDroite.cshtml")
        <div class="control-sidebar-bg"></div>
    </div>




    <!-- jQuery 2.2.3 -->
    <script src="~/assetsWeb/plugins/jQuery/jquery-2.2.3.min.js"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="https://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
  $.widget.bridge('uibutton', $.ui.button);
    </script>
    <!-- Bootstrap 3.3.6 -->
    <script src="~/assetsWeb/bootstrap/js/bootstrap.min.js"></script>
    <!-- Morris.js charts -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js"></script>
    <script src="~/assetsWeb/plugins/morris/morris.min.js"></script>
    <!-- Sparkline -->
    <script src="~/assetsWeb/plugins/sparkline/jquery.sparkline.min.js"></script>
    <!-- jvectormap -->
    <script src="~/assetsWeb/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="~/assetsWeb/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <!-- jQuery Knob Chart -->
    <script src="~/assetsWeb/plugins/knob/jquery.knob.js"></script>
    <!-- daterangepicker -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.11.2/moment.min.js"></script>
    <script src="~/assetsWeb/plugins/daterangepicker/daterangepicker.js"></script>
    <!-- datepicker -->
    <script src="~/assetsWeb/plugins/datepicker/bootstrap-datepicker.js"></script>
    <!-- Bootstrap WYSIHTML5 -->
    <script src="~/assetsWeb/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js"></script>
    <!-- Slimscroll -->
    <script src="~/assetsWeb/plugins/slimScroll/jquery.slimscroll.min.js"></script>
    <!-- FastClick -->
    <script src="~/assetsWeb/plugins/fastclick/fastclick.js"></script>
    <!-- AdminLTE App -->
    <script src="~/assetsWeb/dist/js/app.min.js"></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    <script src="~/assetsWeb/dist/js/pages/dashboard.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="~/assetsWeb/dist/js/demo.js"></script>
</body>
</html>
